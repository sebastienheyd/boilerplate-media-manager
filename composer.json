{
    "name": "sebastienheyd/boilerplate-media-manager",
    "description": "Media Manager for sebastienheyd/boilerplate",
    "keywords": ["laravel", "boilerplate", "media", "file", "manager", "library", "plugin", "package"],
    "type": "library",
    "require": {
        "intervention/image": "^2.4",
        "sebastienheyd/boilerplate": "^6.1.8|dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^4.0",
        "doctrine/dbal": "^2.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Sebastienheyd\\BoilerplateMediaManager\\": "src/"
        }
    },
    "suggest": {
        "sebastienheyd/boilerplate-email-editor": "Visual editor for e-mails"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\BoilerplateMediaManager\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
